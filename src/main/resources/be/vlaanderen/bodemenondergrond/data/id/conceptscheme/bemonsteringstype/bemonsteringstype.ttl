        skos:inScheme   cs:bemonsteringsprocedure;
        skos:member     ct:vloeistof , ct:ongeroerd , ct:geroerd;
        skos:prefLabel  "Collectie van bemonsteringsprocedures."@nl .

ct:ongeroerd  rdf:type     skos:Concept;
